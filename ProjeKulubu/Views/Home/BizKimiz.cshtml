                <p>E-maile Gönder <a href="mailto:info@projekulubuturkiye.com"><strong>info@projekulubuturkiye.com</strong></a></p>
                <div class="internship">
                    <h4>Başlık 2</h4>
                    <ul class="arrow no-border">
                        <li>Liste Metin 1</li>
                        <li>Liste Metin 2</li>
                        <li>Liste Metin 3</li>
                    </ul>
                </div>
            </div>
        </div>
        <!--right side menu-->
        <div class="col-md-3 col-sm-4 col-md-pull-9 col-sm-pull-8">
            @Html.Partial("HakkimizdaMenu")
        </div> <!--end side menu-->
    </div> <!-- end of row-->
</section>


