        <div class="col-md-9 col-sm-8 col-md-push-3 col-sm-push-4 padding-b20-media">
            <!--start body content-->
            <img src="/Content/pikocon_html/images/career-banner.jpg" class="img-responsive" alt="carer banner" />
            <div class="padding-t25">
                <!--start careers -->
                <h4>Başlık 1</h4>
                <p>Metin 1</p>
                <p>Metin 2</p>
