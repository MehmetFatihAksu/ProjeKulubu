
@{
    ViewBag.Title = "MusteriMemnuniyeti";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="banner margin-t150">
    <div class="container page-banner">
        <div class="col-sm-6 col-md-6">
            <h1>Müşteri Memnuniyeti</h1>
        </div>
        <div class="col-sm-6 col-md-6">
            <ol class="breadcrumb pull-right">
                <li><a href="@Url.Action("Index","Home")">Ana Sayfa</a></li>
                <li><a href="@Url.Action("Vizyonumuz","Home")">Hakkımızda</a></li>
                <li><a href="@Url.Action("MusteriMemnuniyeti","Home")">Bireysel</a></li>
                <li class="active">Müşteri Memnuniyeti</li>
            </ol>
        </div>
    </div>
</section>

<section class="container">
    <div class="row padding-tb50">
        <div class="col-md-9 col-sm-8 col-md-push-3 col-sm-push-4 padding-b20-media">
            <!--start body content-->
            <div class="padding-b25">
                <div class="piko-title4 widget nom">
                    <h3><span class="line"></span><span class="text">Müşteri Memnuniyeti</span></h3>
                </div>
                <p class="padding-t10">Paragraf</p>
            </div>
            <div class="padding-t25">
                <!--start history time line-->
                <div class="row process">
                    <div class="col-md-6">
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".25s">
                            <div class="media-left">
                                <img src="/Content/pikocon_html/images/icon/safety.png" alt="feature">
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 1</h4>
                                <p>Başlık 1 Paragraf</p>
                            </div>
                        </div>
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".35s">
                            <div class="media-left">
                                <img src="/Content/pikocon_html/images/icon/worker.png" alt="feature">
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 2</h4>
                                <p>Başlık 2 Paragraf</p>
                            </div>
                        </div>
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".45s">
                            <div class="media-left">
                                <img src="/Content/pikocon_html/images/icon/quality.png" alt="feature">
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 3</h4>
                                <p>Başlık 3 Paragraf</p>
                            </div>
                        </div>
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".45s">
                            <div class="media-left process-icon">
                                <i class="fa fa-building-o" aria-hidden="true"></i>
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 4</h4>
                                <p>Başlık 4 Paragraf</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".25s">
                            <div class="media-left process-icon">
                                <i class="fa fa-bullseye" aria-hidden="true"></i>
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 5</h4>
                                <p>Başlık 5 Paragraf</p>
                            </div>
                        </div>
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".35s">
                            <div class="media-left">
                                <img src="/Content/pikocon_html/images/icon/worker.png" alt="feature">
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 6</h4>
                                <p>Başlık 6 Paragraf</p>
                            </div>
                        </div>
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".45s">
                            <div class="media-left">
                                <a href="#">
                                    <img src="/Content/pikocon_html/images/icon/engineer.png" alt="feature">
                                </a>
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 7</h4>
                                <p>Başlık 7 Paragraf</p>
                            </div>
                        </div>
                        <div class="media  wow slideInUp" data-wow-duration="2s" data-wow-delay=".45s">
                            <div class="media-left">
                                <img src="/Content/pikocon_html/images/icon/process06.png" alt="feature">
                            </div>
                            <div class="media-body">
                                <h4 class="media-heading">Başlık 8</h4>
                                <p>Başlık 8 Paragraf</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--right side menu-->
        <div class="col-md-3 col-sm-4 col-md-pull-9 col-sm-pull-8">
            @Html.Partial("HakkimizdaMenu")
        </div> <!--end side menu-->
    </div> <!-- end of row-->
</section>